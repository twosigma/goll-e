{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": 1,
    "no-trailing-spaces": 1,
    "strict": 1,
    // Turn off error/warning for dangling underscores. We're ok with them.
    "no-underscore-dangle": 0,
    "no-undef": 1,
    "no-shadow": 1,
    "no-unused-vars": 1,
    "no-multi-spaces": 1,
    "key-spacing" : 1
  }
}
